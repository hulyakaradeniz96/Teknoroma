@model List<Teknoroma.MODEL.Entity.Product>
@{
    ViewBag.Title = "SupplierActivities";
    Layout = "~/Views/UnitHead/Shared/Shared.cshtml";
}


<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Sales Tracking</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Title</th>
                        <th>Sales</th>
                        <th>Premium Salary</th>
                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in Model)
                    {
                        if (item.CreatedDate.ToString().Contains(DateTime.Now.Month.ToString()))
                        {
                            <tr>
                                <td>@item.Name</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
